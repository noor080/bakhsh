      "cTitle": "الفصل السابع",
      "cpartOne": "\u0635\u0642\u0631:\u0642\u0627\u0639\u062F \u0628\u064A\u0641\u0643\u0631 \u0641\u064A \u0634\u0647\u062F \u0648\u0643\u0644 \u062D\u0631\u0643\u0647 \u0628\u062A\u0639\u0645\u0644\u0647\u0627 \u064A\u0636\u062D\u0643 \u0639\u0644\u064A\u0647\u0627 \u0648\u0642\u0627\u0644 \u0641\u0648\u0642 \u064A \u0635\u0642\u0631 \u0627\u0646\u0627 \u0645\u0628\u062D\u0628\u0634 \u0647\u064A \u0645\u062C\u0631\u062F \u0628\u062A \u062A\u0627\u0641\u0647\u0647 \u0648\u0627\u0646\u0627 \u0647\u0639\u0631\u0641\u0647\u0627 \u0627\u0632\u0627\u064A \u062A\u0637\u0627\u0648\u0644 \u0639\u0644\u064A \u0632\u0639\u064A\u0645 \u0627\u0644\u0645\u0627\u0641\u064A\u0627 \u0628\u0623\u0643\u0645\u0644\u0647.\n\u0641\u0627\u0631\u0633 \u062F\u062E\u0644: \u0627\u062D\u0645 \u062F\u0644\u0648\u0642\u062A\u064A \u0627\u062D\u0646\u0627 \u0645\u062D\u062A\u0627\u062C\u064A\u0646 \u0633\u0643\u0631\u062A\u064A\u0631\u0629 \u0648\u0627\u0646\u062A \u0645\u0634\u064A\u062A \u0633\u0643\u0631\u062A\u064A\u0631\u0629 \u0644\u0644\u0645\u0642\u0631 \u0627\u0644\u062A\u0627\u0646\u064A \u0647\u0646\u0639\u0645\u0644 \u0627\u064A.\n\u0635\u0642\u0631:\u0631\u0646 \u0639\u0644\u064A \u0645\u0631\u0627\u062F \u0648\u062E\u0644\u064A \u064A\u062C\u064A \u0647\u0648 \u0648\u0627\u0644\u064A \u0627\u0633\u0645\u0647\u0627 \u0634\u0647\u062F \u062F\u064A.\n\u0641\u0627\u0631\u0633:\u062A\u0645\u0627\u0645.\n\u0634\u0647\u062F:\u0647\u064A\u062D\u062D\u062D\u062D\u062D\u062D \u064A \u0628\u0634\u0631 \u0623\u062E\u064A\u0631\u0627\u064B \u062C\u0628\u062A \u0627\u0644\u0637\u0628\u0644\u0647 \u064A\u0644\u0627 \u0628\u0642\u0627 \u0623\u0645\u0627 \u0646\u0634\u0648\u0641.\n\u0645\u0631\u0627\u062F:\u064A\u0644\u0627 \u064A \u0634\u0647\u062F \u0639\u0634\u0627\u0646 \u0646\u0631\u0648\u062D \u0646\u0644\u0628\u0633 \u0639\u0634\u0627\u0646 \u0646\u0631\u0648\u062D \u0634\u0631\u0643\u0647.\n\u0634\u0647\u062F \u0628\u0641\u0631\u062D\u0647:\u0628\u062C\u062F \u0647\u0646\u0631\u0648\u062D \u0627\u0644\u0646\u0647\u0627\u0631\u062F\u0647 \u0628\u062C\u062F\u062F.\n\u0645\u0631\u0627\u062F \u0628\u0634\u0643:\u0627\u0647 \u0641\u064A \u0627\u064A \u0645\u0628\u0633\u0648\u0637\u0647 \u0644\u064A \u0627\u0648\u0639\u064A \u062A\u0643\u0648\u0646\u064A \u0647\u062A\u0639\u0645\u0644\u064A \u062D\u0631\u0643\u0647 \u0643\u062F\u0627 \u0648\u0644\u0627 \u0643\u062F\u0627.\n\u0634\u0647\u062F \u0628\u062C\u062F\u064A\u0647:\u0648\u0627\u0646\u0627 \u0647\u0639\u0645\u0644 \u0627\u064A\u0647 \u064A \u0639\u0646\u064A \u064A\u0644\u0627 \u064A \u0628\u062A \u064A \u0643\u064A\u0627\u0646 \u0627\u0646\u062A\u064A \u0648\u0646\u062C\u0645\u0647 \u0639\u0634\u0627\u0646 \u062A\u064A\u062C\u0648\u0627 \u0645\u0639\u0627\u064A\u0627.\n\u0645\u0631\u0627\u062F:\u0647\u0648 \u062F\u0631\u064A\u0645 \u0628\u0631\u0643 \u062F\u0627 \u0648\u0644\u0627 \u0627\u064A \u062F\u064A \u0634\u0631\u0643\u0647 \u0648\u0634\u063A\u0644.\n\u0646\u062C\u0645\u0647 \u0648\u0643\u064A\u0627\u0646: \u0641\u0639\u0644\u0627\u064B \u0627\u064A \u0627\u0644\u064A \u0647\u064A\u062C\u0628\u0646\u0627 \u0645\u0639\u0627\u0643\u064A.\n\u0634\u0647\u062F:,\u0647\u0648\u0631\u064A\u0643\u0648\u0627 \u0634\u0631\u0643\u0647 \u0628\u0633 \u0648\u0628\u0639\u062F \u0643\u062F\u0627 \u0627\u0645\u0634\u0648\u0627 .\n\u0637\u0628\u0639\u0622 \u062F\u064A \u0634\u0647\u062F \u0648\u0627\u0644\u0627\u062C\u0631 \u0639\u0644\u064A \u0627\u0644\u0644\u0647 \u0648\u0643\u0644\u0645\u0647\u0627 \u0647\u0648 \u0627\u0644\u064A \u0645\u0634\u064A \u0648\u0643\u0644 \u0648\u0627\u062D\u062F \u0631\u0648\u062D \u064A\u062C\u0647\u0632 \u0639\u0634\u0627\u0646 \u064A\u0631\u0648\u062D.\n\n\n\u0641\u0627\u0631\u0633 \u0628\u062A\u0641\u0643\u064A\u0631:\u064A\u0627\u0647\u0627\u0627\u0627 \u0644\u0648 \u0628\u062A \u0634\u0647\u062F \u062A\u062E\u0644\u064A \u0635\u0642\u0631 \u064A\u062D\u0628\u0647\u0627 \u0647\u064A\u0628\u0642\u0627 \u064A \u062C\u0648\u0646 \u062C\u0648\u0646.\n\u0635\u0642\u0631 \u062F\u062E\u0644:\u0645\u0634 \u0647\u062D\u0628\u0647\u0627 \u064A \u0641\u0627\u0631\u0633 \u0648\u0644\u0627 \u0647\u064A \u0648\u0644\u0627 \u063A\u064A\u0631\u0647\u0627 \u0627\u0646\u0627 \u0642\u0644\u0628\u064A \u0645\u064A\u062A \u0648\u0628\u0639\u062F \u0627\u0644\u0639\u064A\u062F \u0627\u0646 \u0634\u0627\u0621 \u0627\u0644\u0644\u0647 \u0647\u0646\u0637\u0644\u0639 \u0639\u0644\u064A \u0627\u0645\u0631\u064A\u0643\u0627 \u0639\u0634\u0627\u0646 \u0647\u0646\u062A\u0641\u0642 \u0645\u0639 \u0648\u0644\u0633\u0646 \u0639\u0644\u064A \u0634\u062D\u0646\u0647.\n\u0641\u0627\u0631\u0633:\u0627\u0634\u0637\u0627 \u0627\u0646\u0627 \u0637\u0627\u0644\u0639 \u0628\u0631\u0627 \u0641\u064A \u062C\u0627\u0631\u062F \u0648\u062C\u0627\u064A.\n\u0635\u0642\u0631: \u0627\u062A\u0641\u0636\u0644 \u0645\u0631\u0627\u062F \u0644\u0645\u0627 \u064A\u062C\u064A \u0627\u0628\u0639\u062A\u0647.\n\u0641\u0627\u0631\u0633: \u0645\u0627\u0634\u064A.\n\u0634\u0647\u062F:\u0644\u0628\u0633\u062A \u0648\u062C\u0647\u0632\u062A \u0648\u0644\u0628\u0633\u062A \u0641\u0633\u062A\u0627\u0646 \u0645\u0646 \u0644\u0648\u0646 \u0627\u0644\u0627\u0632\u0631\u0642 \u0648\u062D\u062C\u0627\u0628 \u0627\u0628\u064A\u0636 \u0645\u0634\u062C\u0631 \u0648\u0643\u0627\u0646\u062A \u062C\u0645\u064A\u0644\u0647 \u0627\u0648\u064A.\n\u0645\u0631\u0627\u062F \u0628\u062A\u0635\u0641\u064A\u0631\u0629:\u0627\u064A \u0627\u0644\u062C\u0645\u0631\u0631\u0631\u0631\u0631 \u062F\u0627 \u064A \u0628\u062A \u064A\u0627\u0634\u0647\u0648\u062F.\n\u0634\u0647\u062F \u0628\u063A\u0645\u0632\u0629:\u0627\u0646\u062A \u0627\u064A \u062C\u0645\u0631 \u062F\u0627 \u064A\u0627\u0648\u0627\u062F \u064A \u0645\u064A\u0645\u0648.\n\u0645\u0631\u0627\u062F:\u0647\u064A \u0627\u064A \u0634\u0646\u0637\u0647 \u0643\u0628\u064A\u0631\u0629 \u062F\u064A \u0641\u064A\u0647\u0627 \u0627\u064A .\n\u0634\u0647\u062F \u0628\u0627\u0644\u062E\u0628\u0637\u0647: \u0639\u0627\u0631\u0641 \u0637\u0628\u0644\u0647 \u0627\u0644\u064A \u062C\u0628\u062A\u0647\u0627 \u0627\u062A\u0641\u0642\u0646\u0627 \u0627\u0646\u0627 \u0648\u0646\u062C\u0645\u0647 \u0648\u0643\u064A\u0627\u0646 \u0644\u0646\u0627 \u0627\u062E\u0644\u0635 \u0647\u0637\u0644\u0639 \u0645\u0639\u0627\u0647\u0645 \u0639\u0644\u064A \u062F\u0627\u0631 \u0627\u0644\u0627\u064A\u062A\u0627\u0645 \u0648\u0646\u062D\u062A\u0641\u0644 \u0648\u0643\u062F\u0627.\n\u0645\u0631\u0627\u062F:\u0645\u0627\u0634\u064A \u064A\u0644\u0627.\n\u0648\u0643\u064A\u0627\u0646 \u0648\u0646\u062C\u0645\u0647 \u0630\u0647\u0628\u0648\u0627 \u0645\u0639 \u0634\u0647\u062F \u0648\u0645\u0631\u0627\u062F \u0627\u0644\u064A \u0634\u0631\u0643\u0647 \u0648\u0641\u064A \u062F\u062E\u0644\u0647\u0645 \u0643\u064A\u0627\u0646 \u062E\u0628\u0637\u062A \u0641\u064A \u0641\u0627\u0631\u0633.\n\u0643\u064A\u0627\u0646:\u0627\u0633\u0641\u0647 \u0627\u0633\u0641\u0647 \u062E\u0627\u0644\u0635 \u0645\u0634 \u0643\u0627\u0646 \u0642\u0635\u062F\u064A.\n\u0641\u0627\u0631\u0633 \u0628\u064A\u0631\u0641\u0639 \u0639\u064A\u0646\u0648\u0627: \u0628\u0635 \u0644\u064A\u0647\u0627 \u0627\u0648\u064A.\n\u0643\u064A\u0627\u0646:\u0628\u0635\u062A \u0644\u064A\u0647 \u0628\u062E\u062C\u0644 \u0648\u0642\u0627\u0644\u062A \u0627\u0644\u0644\u064A\u0644\u064A\u0647 \u0627\u0644\u0645\u0646\u064A\u0644\u0647 \u0645\u0646 \u0627\u0644\u0627\u0648\u0644 \u062F\u064A \u0628\u0633 \u0627\u0644\u0631\u0627\u062C\u0644 \u0645\u0648\u0632 \u0635\u0631\u0627\u062D\u0647 \u0648\u0642\u0627\u0644\u062A \u0627\u062D\u0645 \u0627\u0646\u0627 \u0627\u0633\u0641\u0647 \u062D\u0636\u0631\u062A\u0643.\n\u0641\u0627\u0631\u0633:\u0627\u0646\u062A \u062A\u0639\u0645\u0644 \u0627\u0644\u064A \u0627\u0646\u062A \u0639\u0627\u0648\u0632\u0629 \u064A \u062C\u0645\u064A\u0644.\n\u0643\u064A\u0627\u0646: \u0634\u0643\u0631\u0622.\n\u0641\u0627\u0631\u0633:\u062C\u0627\u064A\u0647 \u0645\u0639 \u0645\u064A\u0646 \u0647\u0646\u0627 \u0631\u062F \u0645\u0631\u0627\u062F \u0648\u0642\u0627\u0644 \u062C\u0627\u064A\u0647 \u0645\u0639 \u0634\u0647\u062F \u062A\u0634\u0648\u0641 \u0634\u0631\u0643\u0647 \u0627\u0644\u064A \u0634\u0647\u062F \u0634\u063A\u0627\u0644\u0647 \u0641\u064A\u0647\u0645.\n\u0641\u0627\u0631\u0633:\u0627\u064A \u064A \u0639\u0645 \u0645\u0631\u0627\u062F \u0627\u0646\u062A \u0634\u0627\u0642\u0637 \u0628\u0646\u0627\u062A \u0645\u0635\u0631 \u0627\u0648\u0645\u0627\u0644 \u0641\u064A\u0646 \u0634\u0647\u062F.\n\u0645\u0631\u0627\u062F:\u0628\u064A\u0628\u0635 \u0648\u0631\u0627\u064A\u0627 \u0627\u0647\u064A \u0648\u0644\u0633\u0647 \u0628\u064A\u0628\u0635 \u0645\u0644\u0642\u0627\u0634 \u062D\u062F.\n\u0634\u0647\u062F:\u0648\u0642\u0641\u062A \u0641\u064A \u0646\u0635 \u0627\u0644\u0634\u0631\u0643\u0647 \u0639\u0644\u064A \u0645\u0643\u062A\u0628 \u0648\u0642\u0627\u0644\u062A \u0627\u062C\u0645\u0639\u0648\u0627 \u064A \u0627\u0647\u0627\u0644\u064A \u0627\u0644\u0628\u0644\u062F \u0627\u0644\u0643\u0631\u0627\u0645 \u0627\u0646\u0627 \u0634\u0647\u062F \u0634\u062E\u0644\u0639\u0647 \u0628\u0646\u062A \u0645\u062D\u062A\u0631\u0645\u0647 \u062C\u062F\u0627\u0627\u0627 \u062D\u0628\u062A \u062A\u0634\u062E\u0644\u0639\u0643\u0648\u0627 \u0641\u064A \u0627\u0644\u0639\u064A\u062F \u0648\u062A\u062D\u0633\u0633\u0643\u0648\u0627 \u0628\u064A.\n\u0643\u0644 \u0627\u0644\u0645\u0648\u0627\u0638\u0641\u064A\u0646 \u0648\u0627\u0642\u0641\u064A\u0646 \u0647\u064A\u0645\u0648\u062A\u0648\u0627 \u0645\u0646 \u0636\u062D\u0643 \u0648\u0627\u0646\u062F\u0645\u062C\u0648\u0627 \u0627\u0643\u062A\u0631.\n\u0644\u0645\u0627 \u0634\u0647\u062F:\u0637\u0644\u0639\u062A \u0627\u0644\u0637\u0627\u0628\u0644\u0647 \u0648\u0628\u0637\u0628\u0644 \u0648\u0628\u062A\u0642\u0648\u0644\n\u0623\u0647\u0644\u0627 \u0622\u0647\u0644\u0627 \u0628\u0627\u0644\u0639\u064A\u062F \u0645\u0631\u062D\u0628 \u0645\u0631\u062D\u0628 \u0628\u0627\u0644\u0639\u064A\u062F\n\u0647\u064A \u0647\u064A \u0647\u064A \u0647\u0647\u0647\u064A \u0647\u064A\n\u0627\u0644\u0639\u064A\