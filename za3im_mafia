64A \u062E\u0644\u064A\u062A \u0627\u0644\u0648\u0644\u0627 \u0628\u0642\u0627 \u0634\u0628\u0647 \u062A\u0645\u062B\u0627\u0644.\n\u0641\u0627\u0631\u0633:\u0647\u0646\u062C\u0648\u0632 \u064A \u0634\u0647\u062F \u0643\u0645\u0627\u0646 \u0634\u0647\u0631.\n\u0634\u0647\u062F \u0628\u0641\u0631\u062D\u0647:\u0645\u0628\u0631\u0648\u0643 \u0645\u0628\u0631\u0648\u0643 \u0627\u0646\u0627 \u0647\u0642\u0641\u0644 \u0628\u0642\u0627 \u0627\u0634\u0648\u0641 \u0645\u0631\u0627\u062F.\n\u0645\u0631\u0627\u062F \u0628\u0641\u0631\u062D\u0647:\u0645\u0633\u0643 \u0627\u064A\u062F \u0634\u0647\u062F \u0648\u0628\u064A\u0631\u0642\u0635 \u0648\u0628\u064A\u0642\u0648\u0644\u0627\u0644\u0646\u0647\u0627\u0631\u062F\u0647 \u0641\u0631\u062D \u064A\u0627 \u062C\u062F\u0639\u0627\u0646\n\u0639\u0627\u064A\u0632 \u0643\u0644\u0648 \u064A\u0628\u0642\u0649 \u062A\u0645\u0627\u0645\n\u0644\u0642\u064A\u062A\u0647\u0627 \u0645\u0627\u0634\u064A\u0629 \u0645\u0634\u064A\u062A \u0648\u0631\u0627\u0647\u0627\n\u0642\u0644\u062A \u0644\u0627\u0632\u0645 \u0623\u0639\u064A\u0634 \u0645\u0639\u0627\u0647\u0627\n\u0639\u0627\u0631\u0641\u064A\u0646 \u0642\u0644\u062A\u0644\u0647\u0627 \u0627\u064A\u0647 \u061F\n\u0628\u0639\u062F \u0625\u0632\u0646 \u0633\u064A\u0627\u062F\u062A\u0643",
      "cpartTwo": "\u062F\u0647 \u0627\u0646\u0627 \u0645\u0639\u062C\u0628 \u0628\u0633\u0639\u0627\u062F\u062A\u0643\n\u0645\u0645\u0643\u0646 \u0623\u0643\u0644\u0645 \u0637\u0646\u0637\n\u064A\u0645\u0643\u0646 \u0631\u0628\u0646\u0627 \u064A\u0647\u062F\u064A\u0646\u064A\n\u0648\u0627\u0643\u0645\u0644 \u0646\u0635 \u062F\u064A\u0646\u064A\n\u0648\u0627\u0628\u0637\u0644 \u0627\u062A\u0646\u0637\u0637\n\u0648\u0627\u062A\u062C\u0648\u0632.\n\u0634\u0647\u062F \u0628\u062A\u0631\u0642\u0635:\u0648\u0635\u0648\u062A \u0636\u062D\u0643\u062A\u0647\u0627 \u0631\u0646\u064A\u0646 \u0641\u064A \u0628\u064A\u062A \u0628\u062A\u0642\u0648\u0644 \u0623\u0646\u0627 \u062E\u0644\u0627\u0635 \u0647\u062A\u062C\u0648\u0632 \u0648\u0647\u0628\u0637\u0644 \u0623\u0628\u0635 \u0639\u0627\u0644\u0628\u0646\u0627\u062A\n\u0648\u0647\u0638\u0628\u0637 \u0646\u0641\u0633\u064A \u0648\u0623\u0628\u0637\u0644 \u0623\u0642\u0636\u064A\u0647\u0627 \u0625\u0634\u062A\u063A\u0627\u0644\u0627\u062A\n\u0623\u0646\u0627 \u062E\u0644\u0627\u0635 \u0647\u062A\u062C\u0648\u0632 \u0648\u0647\u0628\u0637\u0644 \u0623\u0628\u0635 \u0639\u0627\u0644\u0628\u0646\u0627\u062A\n\u0648\u0647\u0638\u0628\u0637 \u0646\u0641\u0633\u064A \u0648\u0623\u0628\u0637\u0644 \u0623\u0642\u0636\u064A\u0647\u0627 \u0625\u0634\u062A\u063A\u0627\u0644\u0627\u062A\n\u0631\u0642\u0635\u0646\u064A \u0639\u0627\u0644\u0648\u0627\u062D\u062F\u0629\u060C.\n\u0645\u0631\u0627\u062F:\u0634\u0627\u0644 \u0634\u0647\u062F \u0648\u0644\u0641 \u0628\u064A\u0647\u0627 \u0648\u0642\u0627\u0644\u0631\u0642\u0635\u0646\u064A \u0639\u0627\u0644\u0648\u0627\u062D\u062F\u0629\u060C \u0645\u0634 \u0647\u0645\u0634\u064A \u0648\u0631\u0627 \u0648\u0627\u062D\u062F\u0629\n\u0631\u0642\u0635\u0646\u064A \u0639\u0627\u0644\u0648\u0627\u062D\u062F\u0629\u060C \u0645\u0634 \u0647\u0645\u0634\u064A \u0648\u0631\u0627 \u0648\u0627\u062D\u062F\u0629.\n\n\n\u0634\u0647\u062F \u0628\u0641\u0631\u062D\u0647:\u0646\u0632\u0644\u062A\u064A \u064A \u