      "cTitle": "الفصل السابع عشر",
      "cpartOne": "\u0646\u062C\u0645\u0647:\u0627\u0647\u0627\u0627\u0627\u0627\u0627\u0627 \u064A \u0635\u062F\u0627\u0639\u064A \u0647\u064A\u0641\u0631\u0642\u0639 \u0646\u0641\u0648\u062E\u064A \u064A \u0646\u064A \u0627\u0639\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627 \u064A \u0636\u063A\u0637\u0643 \u0627\u0644\u0639\u0627\u0644\u064A \u064A \u0646\u062C\u0648\u0645 \u0628\u0633 \u062B\u0627\u0646\u064A\u0647 \u0647\u0648 \u0627\u0646\u0627 \u0639\u0646\u062F\u064A \u0636\u063A\u0637 \u0627\u0639\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627 \u0645\u0634 \u0642\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u062F\u0631\u0629.\n\u0643\u064A\u0627\u0646 \u0628\u062A\u062E\u0628\u0637 \u0639\u0644\u064A \u0628\u0627\u0628\u0627:\u0646\u062C\u0645\u0647 \u0627\u0641\u062A\u062D\u064A \u0628\u062A.\n\u0646\u062C\u0645\u0647:\u062D\u0627\u0627\u0627\u0636\u0631 \u064A\u0644\u064A \u0639\u0644\u064A \u0628\u0627\u0628\u0627 \u0647\u0648 \u0627\u0646\u0627 \u0645\u0634 \u0645\u0648\u062D\u062C\u0628\u0647 \u0648\u0644\u0627\u0632\u0645 \u0627\u0644\u0628\u0633\u0633\u0633\u064A.\n\u0643\u064A\u0627\u0646:\u0645\u0648\u062D\u062C\u0628\u0647 \u0627\u0644\u0645\u0647\u0645 \u0627\u0646\u0627 \u0643\u064A\u0627\u0646 \u064A \u062E\u062A\u064A \u0627\u0641\u062A\u062D\u064A.\n\u0646\u062C\u0645\u0647:\u0641\u062A\u062D\u062A \u0648\u0642\u0627\u0644\u062A \u062E\u0634\u064A.\n\u0643\u064A\u0627\u0646 \u0628\u0635\u062F\u0645\u0647:\u0627\u0639\u0648\u0630 \u0628\u0627\u0644\u0644\u0647 \u0645\u064A\u0646 \u062F\u064A \u0641\u064A\u0646 \u0646\u062C\u0645\u0647.\n\u0646\u062C\u0645\u0647: \u0627\u0639\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627 \u0627\u0646\u0627 \u0646\u062C\u0645\u0647 \u0631\u0627\u062D \u062C\u0645\u0627\u0644\u0643 \u064A \u0646\u062C\u0645\u0647.\n\u0643\u064A\u0627\u0646:\u0627\u064A \u0627\u0644\u064A \u0639\u0645\u0644 \u0641\u064A \u0634\u0639\u0631\u0643 \u0648\u0634\u0643 \u0643\u062F\u0627\u0627\u0627\u0627 \u064A \u0646\u062C\u0645\u0647.\n\u0646\u062C\u0645\u0647:\u0643\u0648\u0646\u062A \u0642\u0627\u0639\u062F\u0647 \u0639\u0644\u064A \u064A\u0648\u062A\u064A\u0648\u0628 \u0628\u062A\u0641\u0631\u062C \u0639\u0644\u064A \u0631\u0648\u062A\u064A\u0646 \u0627\u0644\u064A\u0648\u0645\u064A \u0641 \u0642\u0627\u0644\u062A \u0627\u0643\u0648\u064A \u0634\u0639\u0631\u0643 \u0628\u0627\u0644\u0645\u0643\u0648\u0647 \u0628\u062A\u0627\u0639\u062A \u0627\u0644\u0647\u062F\u0648\u0645 \u0634\u0639\u0631\u0643 \u0647\u064A\u0628\u0642\u0627 \u062D\u0631\u064A\u0631 \u0644\u0644\u0627\u0633\u0641 \u0634\u0639\u0631\u064A \u0634\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0637 \u0648\u0637\u0628\u0644\u0647 \u0631\u0627\u0633\u064A \u0627\u062A\u062D\u0631\u0642\u062A \u0648\u0639\u0646\u062F\u064A \u0635\u062F\u0627\u0627\u0627\u0627\u0627\u0627\u0639.\n\n\n\u0643\u064A\u0627\u0646 \u0628\u0635\u062F\u0645\u0647:\u0627\u0646\u0647\u0627\u0631 \u0627\u0628\u064A\u0636 \u0628\u0645\u0643\u0648\u0647 \u0647\u062F\u0648\u0645 \u062A\u0639\u0645\u0644\u064A \u0634\u0639\u0631\u0643 \u0637\u0628 \u0648\u0634\u0643 \u0645\u0646 \u0627\u064A.\n\u0646\u062C\u0645\u0647:\u0635\u0628\u0631\u0643 \u0639\u0644\u064A\u0627 \u0627\u0646 \u0627\u0644\u0644\u0647 \u0645\u0639 \u0635\u0627\u0628\u0631\u064A\u0646 \u062C\u0628\u062A \u0639\u0633\u0644 \u0646\u062D\u0644 \u0648\u062F\u0645 \u063A\u0632\u0627\u0627\u0644 \u0639\u0634\u0627\u0646 \u0627\u062D\u0645\u0631 \u0648\u0634\u064A \u0641\u064A \u0634\u0645\u0633\u0647 \u0627\u0644\u0645\u0647\u0645 \u062D\u0637\u064A\u062A\u0648\u0627 \u0639\u0644\u064A \u0648\u0634\u064A \u0648\u0646\u0633\u064A\u062A \u0639\u0634\u0627\u0646 \u0643\u0648\u0646\u062A \u0628\u062A\u0641\u0631\u062C \u0639\u0644\u064A \u0645\u0633\u0644\u0633\u0644 \u0627\u0632\u0647\u0627\u0631 \u0627\u0644\u062D\u0632\u064A\u0646\u0647 \u0648\u0643\u0648\u0646\u062A \u0628\u0639\u064A\u0637\u0637\u0637 \u0627\u0644\u0645\u0647\u0645 \u0644\u0642\u064A\u062A \u062D\u0627\u062C\u0647 \u0628\u062A\u0642\u0631\u0635\u0646\u064A \u0648\u0643\u0627\u0646 \u0627\u0644\u0646\u0645\u0644 \u0627\u062A\u0644\u0645 \u0639\u0644\u0649 \u0648\u0634\u064A \u0648\u0627\u0644\u0639\u0633\u0644 \u0644\u0627\u0632\u0642 \u0639\u0644\u064A \u0648\u0634\u064A \u062C\u0627\u0627\u0627\u0645\u062F \u0643\u0648\u0646\u062A \u0628\u0634\u064A\u0644 \u0641\u064A \u0644\u0645\u0627 \u0634\u0627\u0644 \u062C\u0644\u062F \u0648\u0634\u064A \u0628\u062F\u0644 \u0645 \u0627\u0643\u0648\u0646 \u062D\u0645\u0631\u0627 \u0641\u064A \u0634\u0645\u0633\u0647 \u0627\u062A\u0633\u0644\u062E\u062A \u062F\u0627\u0626\u0645\u0627 \u0648\u0627\u0628\u062F\u0627 \u0627\u0639\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627.\n\u0643\u064A\u0627\u0646:\u0627\u0644\u062D\u0642\u0648\u0648\u0648\u0648\u0648\u0648\u0648\u0646\u064A \u0647\u0645\u0648\u062A \u0645\u0646 \u0636\u062D\u0643 \u0646\u0627\u0642\u0635\u0647 \u0639\u0645\u0631 \u0641\u064A \u062D\u062F \u064A\u0639\u0645\u0644 \u0643\u062F\u0627 \u0641\u064A \u0646\u0641\u0633\u0648\u0627 \u0627\u0646\u062A\u064A \u0648\u0634\u0647\u062F \u0647\u062A\u0645\u0648\u062A\u0646\u064A \u0646\u0627\u0642\u0635\u0647 \u0639\u0645\u0631.\n\u0646\u062C\u0645\u0647:\u0639\u064A\u0628 \u064A \u0628\u0646\u062A \u062F\u064A \u0645\u062E\u0627\u0628\u0631\u0627\u062A \u0648\u0645\u0645\u0643\u0646 \u062A\u0627\u062E\u062F\u0646\u0627 \u0641\u064A \u0631\u062C\u0644\u064A\u0646.\n\u0643\u064A\u0627\u0646 \u0628\u0635\u062F\u0645\u0647:\u0627\u062D\u0645 \u0646\u062C\u0645\u0647 \u0641\u064A \u0635\u064A\u062F\u0644\u0647 \u0627\u064A \u064A \u0628\u0646\u062A\u064A \u0627\u0644\u0647\u0628\u0644 \u062F\u0627.\n\u0646\u062C\u0645\u0647:\u0627\u062A\u0648\u0643\u0633\u064A \u0639\u0631\u0641\u062A\u0646\u064A \u0648\u0642\u0627\u0644\u062A\u0644\u064A \u0627\u0646\u0643 \u0639\u0627\u0631\u0641\u0647 \u0639\u0646\u0647\u0627 \u0643\u0644 \u062D\u0627\u062C\u0647 \u0627\u0644\u0645\u0647\u0645 \u0627\u0646\u0627 \u062E\u0627\u064A\u0641\u0647 \u0639\u0644\u064A\u0647\u0627 \u062C\u062F\u0627\u0627\u0627.\n\u0643\u064A\u0627\u0646 \u0628\u062E\u0648\u0641:\u0645\u064A\u0646 \u0642\u0627\u0644\u0643 \u0628\u0633 \u0645\u0634\u0627\u0621 \u0627\u0644\u0644\u0647 \u0645\u0641\u064A\u0634 \u0642\u0627\u0636\u064A\u0647 \u062A\u0637\u0644\u0639 \u0645\u0646\u0647\u0627 \u062E\u0633\u0631\u0627\u0646\u0627 \u0631\u0628\u0646\u0627 \u064A\u062D\u0645\u064A\u0647\u0627.\n\u0635\u0642\u0631:\u0641\u0627\u0627\u0627\u0631\u0633 \u062A\u0639\u0627\u0644\u064A \u0635\u0641\u064A \u0643\u0644 \u0627\u0644\u0641\u0644\u0648\u0633 \u0627\u0644\u064A \u062A\u0628\u0639 \u0627\u0644\u0645\u0627\u0641\u064A\u0627 \u0627\u062A\u0628\u0639\u0631 \u0628\u064A\u0647\u0627 \u0643\u0644\u0647\u0627.\n\u0641\u0627\u0631\u0633 \u0628\u0635\u062F\u0645\u0647: \u0627\u0646\u062A \u0628\u062A\u0642\u0648\u0644 \u0627\u064A \u064A \u0635\u0642\u0631 \u0627\u0646\u062A \u0628\u0627\u064A\u0646 \u0639\u0644\u064A\u0643 \u0627\u062A\u0647\u0628\u0644\u062A.\n\u0635\u0642\u0631: \u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627 \u0641\u0648\u0642\u062A \u0627\u0646\u062A \u0627\u0632\u0627\u064A \u062A\u0643\u0644\u0645 \u0645\u0639\u0627\u064A\u0627 \u0643\u062F\u0627 \u0627\u0646\u062A \u0634\u063A\u0627\u0644 \u0645\u0639\u0627\u064A\u0627 \u0648\u062A\u0646\u0641\u0630 \u0627\u0644\u064A \u0639\u0627\u0648\u0632\u0629.\n\u0641\u0627\u0631\u0633 \u0628\u0635\u062F\u0645\u0647:\u0627\u0644\u0643\u0628\u064A\u0631\u0631\u0631 \u0647\u064A\u0645\u0648\u062A\u0643 \u0648\u0627\u0646\u0627 \u062E\u0627\u064A\u0641 \u0639\u0644\u064A\u0643 \u064A \u0635\u0627\u062D\u0628\u064A \u0627\u0646\u0627 \u0641\u0639\u0644\u0627\u064B \u0634\u063A\u0627\u0644 \u0639\u0646\u062F\u0643 \u0633\u0627\u0639\u0647 \u0648\u0647\u0643\u0648\u0646 \u0645\u0633\u062A\u0642\u0644.\n\u062F\u062E\u0644\u062A \u0634\u0647\u062F \u0648\u0645\u0631\u0627\u062F:\u0642\u0627\u0644\u062A \u0641\u064A \u0627\u064A \u0635\u0648\u062A\u0643\u0648\u0627 \u0639\u0627\u0644\u064A \u064A \u0646\u062C\u0648\u0645.\n\u0635\u0642\u0631:\u0627\u0646\u0627 \u0647\u0633\u0644\u0645 \u0646\u0641\u0633\u064A \u064A \u0641\u0627\u0627\u0627\u0631\u0633 \u0628\u0633 \u0628\u0639\u062F \u0645 \u0627\u0633\u0644\u0645 \u0643\u0628\u064A\u0631 \u0644\u0634\u0631\u0637\u0647 \u0639\u0634\u0627\u0646 \u0647\u0648 \u0633\u0628\u0628\u0628 \u0641\u064A \u062F\u0645\u0627\u0627\u0627\u0627\u0627\u0631 \u0627\u0644\u064A \u0627\u062D\u0646\u0627 \u0641\u064A.\n\u0641\u0627\u0631\u0633 \u0648\u0645\u0631\u0627\u062F \u0628\u0635\u062F\u0645\u0647:\u0627\u064A\u0647 \u0627\u0646\u062A \u0628\u062A\u0642\u0648\u0644 \u0627\u064A\u0647.\n\u0634\u0647\u062F \u0628\u0641\u0631\u062D\u0647:\u0648\u0642\u0627\u0644\u062A \u0645 \u0628\u0646\u0647\u0627\u0627 \u0643\u0648\u0646\u062A \u0639\u0627\u0631\u0641\u0647 \u0627\u0646\u0643 \u0647\u062A\u0639\u0645\u0644 \u0643\u062F\u0627 \u0648\u0627\u0646 \u0634\u064A\u062E \u0647\u064A\u0623\u062B\u0631 \u0639\u0644\u064A\u0643.\n\u0641\u0627\u0631\u0633:\u0627\u0646\u062A \u0639\u0627\u0631\u0641 \u0643\u0628\u064A\u0631 \u0642\u0627\u0644\u064A \u0627\u064A \u064A \u0635\u0642\u0631 \u0648\u0646\u0628\u064A \u064A \u0635\u0642\u0631\u0631\u0631\u0631 \u0628\u0644\u0627\u0634 \u0627\u0646\u0627 \u0645\u0639\u0627\u0643 \u0641\u064A \u0627\u064A \u062D\u0627\u062C\u0647 \u0647\u062A\u0648\u0628 \u0645\u0639\u0627\u0643 \u0648\u0647\u0641\u0648\u0642 \u0628\u0633 \u062A\u0648\u0627\u062C\u0647\u0647 \u0627\u0644\u0643\u0628\u064A\u0631 \u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627\u0644\u0627 \u0645\u0634 \u0647\u0633\u0645\u062D\u0644\u0643.\n\u0634\u0647\u062F:\u0627\u0633\u062A\u0627\u0630 \u0641\u0627\u0631\u0633 \u0639\u0646\u062F\u0647 \u062D\u0642 \u064A \u0627\u0633\u062A\u0627\u0630 \u0635\u0642\u0631.\n\u0645\u0631\u0627\u062F: \u0641\u0639\u0644\u0627\u064B \u0628\u0644\u063A \u0648\u0642\u062F\u0645 \u0627\u0644\u064A \u0645\u0639\u0627\u0643.\n\u0635\u0642\u0631:\u0647\u0647\u0647\u0647\u0647 \u0636\u062D\u0643\u062A\u0648\u0646\u064A \u0643\u0628\u064A\u0631\u0631\u0631\u0631 \u0627\u0646\u0627 \u0634\u063A\u0627\u0644 \u0645\u0639\u0627\u0647 \u0648\u0627\u0646\u0627 \u0639\u064A\u0644 \u0639\u0646\u062F\u064A 19\u0633\u0646\u0647 \u0645\u0634 \u0628\u0633\u0627\u0647\u0644 \u0627\u0646\u0627 \u0631\u0627\u064A\u062D \u0644\u064A\u0647 \u0648\u0627\u0646 \u0634\u0627\u0621 \u0627\u0644\u0644\u0647 \u0647\u064A\u0643\u0648\u0646 \u0645\u0646 \u0645\u0646\u0648\u0631 \u0627\u0644\u0646\u0647\u0627\u0631\u062F\u0647 \u0641\u064A \u0627\u0644\u062D\u0628\u0633.\n\u0634\u0647\u062F \u0628\u0635\u062F\u0645\u0647:\u0635\u0642\u0631\u0631\u0631\u0631\u0631\u0631\u0631\u0631 \u0648\u0642\u0627\u0644\u062A \u0628\u0633\u0631\u0639\u0647 \u0645\u0631\u0627\u062F \u062C\u0647\u0632 \u0627\u0644\u0642\u0648\u0629.\n\u0641\u0627\u0631\u0633:\u0635\u0642\u0631\u0631\u0631\u0631\u0631\u0631 \u0628\u064A\u062C\u0631\u064A \u0648\u0631\u0627\u0647 \u0633\u0645\u0639 \u0634\u0647\u062F \u0644\u0641 \u0648\u0642\u0627\u0644 \u0642\u0648\u0644\u062A\u064A \u0627\u064A.\n\u0634\u0647\u062F:\u0645\u0641\u064A\u0634 \u0648\u0642\u062A \u0644\u062A\u0641\u0633\u064A\u0631\u0631\u0631\u0631\u0631\u0631 \u0627\u062C\u0647\u0632 \u064A \u0645\u0631\u0627\u062F \u0628\u0633\u0631\u0631\u0631\u0631\u0639\u0647.\n\u0645\u0631\u0627\u062F:\u0643\u0644\u0645 \u0627\u0644\u0645\u0642\u062F\u0645 \u0648\u062C\u0647\u0632 \u0627\u0644\u0642\u0648\u0629.\n\u0648\u0634\u0647\u062F:\u0644\u0628\u0633\u062A \u0644\u0628\u0633 \u0627\u0644\u0634\u0631\u0637\u0647 \u0648\u0644\u0628\u0633\u062A \u062E\u0648\u0630\u0629 \u0639\u0644\u064A \u0648\u0634\u0647\u0627 \u0648\u062D\u0637\u062A \u0