      "cpartOne": "\u0634\u0647\u062F \u0628\u063A\u064A\u0638: \u0644\u0641\u062A \u062A\u062A\u0645\u062A\u0645 \u0628\u063A\u064A\u0638 \u0648\u0641\u0627\u062C\u0627\u0629 \u062E\u0628\u0637\u062A \u0641\u064A \u062D\u062F \u0628\u0635\u062A \u0645\u0627\u062A\u062D\u0627\u0633\u0628 \u064A\u0627\u0639\u0645\u0646\u0627 \u0627\u0644\u0644\u0647.\n\u0627\u0644\u0634\u0627\u0628:\u0639\u0644\u064A \u0641\u0643\u0631\u0629 \u0623\u0646\u062A\u0650 \u0627\u0644\u064A \u0643\u0648\u0646\u062A\u064A \u0645\u0627\u0634\u064A\u0647 \u0628\u0635\u064A \u0641\u064A \u0627\u0644\u0627\u0631\u0636 \u0645\u0634 \u0627\u0646\u0627.\n\u0634\u0647\u062F:\u062D\u0644\u0648 \u0645\u062F\u0627\u0645 \u0643\u0648\u0646\u062A \u0628\u0635\u064A \u0639\u0644\u064A \u0627\u0644\u0627\u0631\u0636 \u064A\u0627\u0639\u0646\u064A \u0627\u0646\u062A \u0643\u0648\u0646\u062A \u0634\u064A\u0627\u0641\u0646\u064A \u062B\u062D \u0648\u0644\u0627 \u0645\u0634 \u062B\u062D \u0631\u062F \u0627\u064A\u0647\u0627 \u0627\u0644\u0645\u062C\u0631\u0645.\n\u0634\u0627\u0628: \u0645\u0639\u0627\u0643\u064A \u0627\u0644\u0645\u062C\u0631\u0645 \u0639\u0627\u062F\u0644 \u0627\u0645\u0627\u0645.\n\u0634\u0647\u062F \u0628\u063A\u0636\u0628:\u0644\u0627 \u0627\u0646\u062A \u0643\u0645\u0627\u0646 \u0628\u062A\u063A\u0644\u0637 \u0641\u064A \u0627\u0644\u0645\u0645\u062B\u0644 \u0627\u0644\u0645\u0641\u0636\u0644 \u0639\u0646\u062F\u064A \u0647\u0627\u0627\u0627\u0631 \u0645\u0641\u0648\u062A .\n\u0639\u0627\u062F\u0644 \u0628\u0636\u062D\u0643:\u0645\u0634 \u0642\u0627\u0627\u0627\u0627\u0627\u0627\u062F\u0631 \u0628\u0635\u064A \u064A\u0627\u0633\u062A \u0627\u0646\u0627 \u0627\u0633\u0645\u064A \u0639\u0627\u062F\u0644 \u0627\u0645\u0627\u0645 \u0628\u062C\u062F .\n\u0634\u0647\u062F \u0628\u0647\u0631\u0634\u0647 \u0639\u0644\u064A \u062D\u062C\u0627\u0628\u0647\u0627:\u0648\u0628\u0635\u062A \u0644\u064A\u0647 \u0627\u0647\u0627\u0627\u0627 \u064A\u0644\u0627 \u0627\u062B\u0641\u0647 \u0627\u062B\u0641\u0647 \u0644\u062D\u062F \u0627\u0644\u062A\u0631\u0639\u0647.\n\u0639\u0627\u062F\u0644: \u0643\u0627\u0646 \u0645\u0631\u0643\u0632 \u0641 \u0639\u0646\u064A\u0647\u0627 \u0648\u0637\u0631\u064A\u0642\u0647 \u0643\u0644\u0627\u0645\u0647\u0627 \u0627\u0644\u0637\u0641\u0648\u0644\u064A \u0648\u0642\u0627\u0644 \u0628\u062A\u0648\u0647\u0627\u0646 \u0623\u0646\u062A\u0650 \u062C\u0645\u064A\u0644\u0647 \u0627\u0648\u064A.\n\u0634\u0647\u062F \u0628\u0644\u0627\u0645\u0628\u0627\u0644\u0647: \u0639\u0627\u0631\u0641\u0647 \u064A\u0627\u062E\u0648\u064A\u0627 \u0627\u0628\u0639\u062F \u0628\u0642\u0627 \u0639\u0634\u0627\u0646 \u0627\u0632\u0641\u062A \u0627\u0645\u0634\u064A.\n\u0639\u0627\u062F\u0644 \u0628\u0636\u062D\u0643\u0647:\u064A\u062E\u0631\u0628\u064A\u062A\u0643 \u0641\u0638\u064A\u0639\u0647 \u0628\u0633 \u0642\u0645\u0631.\n\u201C\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D",
      "cpartTwo": "\u0635\u0642\u0631:\u0633\u0627\u064A\u0642 \u0627\u0644\u0639\u0631\u0628\u064A\u0647 \u0648\u062C\u0645\u0628\u0648\u0627 \u0641\u0627\u0631\u0633 \u0648\u0628\u064A\u0642\u0648\u0644 \u0627\u0644\u0634\u062D\u0646\u0647 \u0627\u062A\u0644\u063A\u062A \u0648\u0647\u0646\u0631\u0648\u062D \u0639\u0644\u064A \u0634\u0631\u0643\u0647 \u0648\u0645\u062D\u062A\u0627\u062C\u064A\u0646 \u0645\u062D\u0627\u0633\u0628\u064A\u0646 \u0643\u0648\u064A\u0633\u064A\u0646 \u0648\u0633\u0643\u0631\u062A\u0627\u0631\u064A\u0629.\n\u0641\u0627\u0631\u0633:\u062A\u0645\u0627\u0645 \u0645\u0627\u0634\u064A \u0647\u0646\u0632\u0644 \u0627\u0639\u0644\u0627\u0646 \u0648\u0646\u0634\u0648\u0641 \u0627\u0644\u062F\u0646\u064A\u0627.\n\u0635\u0642\u0631 \u0628\u062A\u0646\u0628\u064A\u0647:\u0628\u0633 \u062E\u0627\u0644\u064A \u0628\u0627\u0644\u0643 \u0639\u0634\u0627\u0646 \u0645\u0634 \u0639\u0627\u0648\u0632\u064A\u0646 \u063A\u0644\u0637.\n\u0641\u0627\u0631\u0633: \u062A\u0645\u0627\u0645 \u064A\u0627\u0628\u0648\u0635.\n\u0641\u0627\u062C\u0623\u0629 \u0635\u0642\u0631 \u0638\u0647\u0631\u062A \u0642\u062F\u0627\u0645\u0647 \u0628\u0646\u062A \u0648\u0641\u064A \u0627\u062E\u0631 \u0644\u062D\u0638\u0647 \u0648\u0642\u0641 \u0648\u0647\u064A \u0648\u0642\u0639\u062A.\n\u0641\u0627\u0631\u0633 \u0628\u062E\u0636\u0647 \u0641\u064A \u0627\u064A \u064A\u0627\u0631\u0628 \u062E\u064A\u0631\n\u0635\u0642\u0631 \u0628\u063A\u0636\u0628:\u0627\u0646\u0627 \u0647\u0648\u0631\u064A\u0647\u0627 \u0627\u0644\u0639\u0627\u0645\u064A\u0647 \u062F\u064A.\n\u0634\u0647\u062F: \u0627\u0639\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627\u0627 \u064A\u0627\u062B\u063A\u0646\u0646\u0647 \u0639\u0644\u064A \u0627\u0644\u0645\u0639\u0627\u0634 \u064A\u0627\u0634\u0647\u0648\u0648\u062F \u064A\u0627\u0644\u064A \u062D\u0638\u0643 \u0645\u0647\u0628\u0628 \u0643\u0648\u0646\u062A \u0628\u0642\u0648\u0644 \u0637\u0627\u0627\u0631 \u0637\u0627\u0631\u0631 \u0627\u0644\u064A \u0648\u0627\u0642\u0641 \u064A\u0628\u0642\u0627 \u062D\u0645\u0627\u0631 \u0627\u062F\u064A\u0646\u064A \u0639\u0634\u0627\u0646 \u0627\u062A\u0646\u0645\u0631\u062A \u0648\u0642\u0639\u062A \u0648\u0641\u0642\u062F\u062A \u0631\u062C\u0644\u064A \u0627\u0644\u0639\u0632\u064A\u0632\u0629.\n\u0641\u0627\u0631\u0633:\u0643\u0627\u0646 \u0647\u0644\u0643\u0627\u0646 \u0645\u0646 \u0636\u062D\u0643 \u0648\u0645\u0634 \u0642\u0627\u062F\u0631 .\n\u0635\u0642\u0631:\u0648\u0627\u0642\u0641 \u0639\u064A\u0646\u0648\u0627 \u0628\u0637\u0644\u0639 \u0634\u0631\u0627\u0631\u0629.\n\u0634\u0647\u062F:\u0641\u0627\u062C\u0623\u0629 \u0636\u062D\u0643\u062A \u0648\u0628\u0635\u062A \u0644\u0641\u0627\u0631\u0633 \u0648\u0642\u0627\u0644\u062A \u0627\u062E\u064A\u0631\u0627 \u062D\u062B\u0644 \u062D\u0627\u062C\u0647 \u062D\u0644\u0648\u0647 \u0641\u064A \u064A\u0648\u0645\u064A \u0648\u0634\u0648\u0641\u062A \u0627\u0644\u0645\u0648\u0632\u0632 \u062F\u0648\u0644 \u0648\u0628\u0635\u062A \u0635\u0642\u0631 \u0648\u0642\u0627\u0644\u062A \u0643\u0627\u0628\u062A\u0646 \u0635\u0646\u0627\u0639\u064A \u062F\u0648\u0644 \u0648\u0644\u0627 \u0646\u0641\u062E \u0642\u0635\u062F\u0647\u0627 (\u0627\u0644\u0639\u0636\u0644\u0627\u062A).\n\u0641\u0627\u0631\u0633:\u0645\u0633\u0643 \u0628\u0637\u0646\u0647 \u0645\u0646 \u0636\u062D\u0643 \u0648\u0642\u0627\u0644 \u0644\u0627\u0644\u0627\u0644\u0627 \u0645\u0634 \u0642\u0627\u062F\u0631 \u0627\u064A \u0627\u0644\u0639\u0633\u0644 \u062F\u0627.\n\u0634\u0647\u062F:\u0642\u0627\u0645\u062A \u0641\u0627\u062C\u0623\u0629 \u0648\u0644\u0641\u062A \u062D\u0648\u0644\u064A\u0646 \u0641\u0627\u0631\u0633 \u0648\u0635\u0642\u0631 \u0648\u0642\u0627\u0644\u062A \u0628\u0645\u0631\u062D \u0648\u0646\u0628\u064A \u0627\u0646\u062A\u0648\u0627 \u0627\u0644\u064A \u0639\u062B\u0644\u0627\u062A \u060C\u0639\u0633\u0644\u0627\u062A \u0647\u0648 \u0641\u064A \u0643\u062F\u0627 \u0627\u0648\u0639\u0627\u0627\u0627 \u064A\u0644\u0627.\n\u0635\u0642\u0631 \u0628\u0639\u0635\u0628\u064A\u0629:\u0623\u0646\u062A\u0650 \u064A\u0627\u0628\u062A \u0623\u0646\u062A\u0650 \u0647\u0628\u0644\u0647 \u0648\u0644\u0627 \u0633\u0627\u064A\u0642\u0647 \u0627\u0644\u0639\u0628\u0637 \u0648\u0644\u0627 \u0623\u0646\u062A\u0650 \u0645\u0646 \u0627\u0644\u0628\u0646\u0627\u062A \u0627\u0644\u064A \u0628\u062A\u0631\u0645\u064A \u0639\u0634\u0627\u0646 \u0627\u0644\u0641\u0644\u0648\u0633 \u0648\u0644\u0627 \u0634\u0642\u0637.\n\u0634\u0647\u062F \u0628\u062A\u0632\u0645\u0631:\u0641\u0643\u0627\u0631\u0646\u064A \u0647\u062F\u0627\u064A\u0642 \u0648\u0627\u0639\u064A\u0637 \u062F\u0644\u0648\u0642\u062A\u064A \u0648\u0627\u062A\u0645\u0633\u0643\u0646 \u062B\u062D \u0644\u0627 \u0644\u0641 \u0643\u062F\u0627 \u062D\u0627\u0633\u0628.\n\u0635\u0642\u0631 \u0648\u0641\u0627\u0631\u0633:\u0644\u0641\u0648\u0627 \u0645\u0644\u0642\u0648\u0634 \u062D\u0627\u062C\u0647.\n\n\n\u0635\u0642\u0631: \u0644\u0633\u0647 \u0647\u064A\u0644\u0641 \u0643\u0627\u0646\u062A \u0634\u0647\u062F \u0645\u0637\u0644\u0639\u0647 \u0634\u0628\u0634\u0628 \u062D\u0645\u0627\u0645 \u0645\u0646 \u0634\u0627\u0646\u0637\u0647 \u0648\u0628\u062A\u0636\u0631\u0628 \u0641\u064A \u0635\u0642\u0631 \u0648\u062A\u0642\u0648\u0644 \u0623\u0646\u0627 \u064A\u062F\u0644\u0639\u062F\u064A \u0628\u0631\u0645\u064A \u0646\u0641\u0633\u064A \u062A\u0639\u0627\u0644\u064A \u0627\u0648\u0631\u064A\u0643 \u0648\u0628\u0635\u062A \u0644\u0641\u0627\u0627\u0631\u0633 \u0648\u0642\u0627\u0644\u062A \u0648\u0633\u0639 \u0627\u0646\u062A \u0627\u0646\u062A \u0639\u062B\u0644 \u0648\u063A\u0645\u0632\u062A.\n\u0641\u0627\u0631\u0633:\u0645\u0643\u0627\u0646\u0634 \u0645\u0639\u0627\u0647\u0627 \u0648\u0642\u0627\u0644 \u064A\u0627\u0644\u0647\u0648\u064A \u064A\u0627\u0639\u0646\u064A \u0639\u0644\u064A\u0643\u064A \u0643\u0648\u0646\u062A\u064A \u062F\u0645\u0643 \u062E\u0641\u064A\u0641 \u064A\u0644\u0627 \u0627\u0644\u0631\u062D\u0645\u0647 \u0639\u0644\u064A \u0631\u0648\u062D\u0643 \u064A\u0627\u0634\u0647\u0648\u0648\u0648\u062F .\n\u0635\u0642\u0631 \u0628\u0639\u0635\u0628\u064A\u0647:\u0645\u0633\u0643 \u0627\u064A\u062F \u0634\u0647\u062F \u062C\u0627\u0645\u062F \u0648\u0642\u0627\u0644 \u0627\u0646\u062A\u064A \u0627\u062A\u0647\u0628\u0644\u062A\u064A \u064A\u0627\u0628\u062A \u0627\u0646\u062A\u064A \u0627\u0632\u0627\u064A \u062A\u062A\u062C\u0631\u0626 \u062A\u0639\u0645\u0644\u064A \u0643\u062F\u0627 \u0645\u0639 \u0635\u0642\u0631 \u0627\u0644\u062D\u062F\u064A\u062F\u064A \u0648\u0632\u0639\u0642 \u0628\u0635\u0648\u062A\u0648\u0627 \u0643\u0644\u0648\u0627 \u0642\u0627\u0644 \u0631\u062F\u064A.\n\u0634\u0647\u062F \u0628\u062E\u0648\u0641:\u0628\u0635\u062A \u0641\u064A \u0639\u0646\u064A\u0647 \u0648\u0639\u064A\u0637\u062A \u0648\u0642\u0627\u0644\u062A \u0627\u064A\u062F\u064A \u0628\u062A\u0648\u062C\u0639\u0646\u064A \u0627\u0648\u064A \u0627\u0648\u064A \u0627\u0648\u064A \u0633\u0628\u0646\u064A.\n\u0635\u0642\u0631 \u0645\u0644\u0627\u0645\u062D\u0648\u0627 \u0628\u062F\u0623\u062A \u062A\u0647\u062F\u064A:\u0648\u0628\u0635 \u0644\u064A\u0647\u0627 \u0627\u0648\u064A \u0643\u0627\u0646 \u0645\u0631\u0643\u0632 \u0645\u0639 \u0639\u064A\u0646\u0647\u0627 \u0628\u0645\u0632\u064A\u062C \u0627\u0644\u0642\u0647\u0648\u0629 \u0648\u0627\u0644\u0646\u0645\u0634 \u0627\u0644\u064A \u0645\u0627\u0644\u064A \u0648\u0634\u0647\u0627 \u0648\u0645\u0644\u0627\u0645\u062D\u0647\u0627 \u0627\u0644\u0628\u0631\u064A\u0626\u0647 \u0648\u0642\u0627\u0644 \u0627\u0646\u062A\u064A \u0645\u064A\u0646 \u0648\u062F\u064A\u0646\u064A \u0644\u0648 \u0645\u0634 \u0628\u0639\u062F\u062A\u064A \u0639\u0646 \u0648\u0634\u064A \u062E\u0644\u0627\u0644 \u062B\u0648\u0627\u0646\u064A \u0647\u0643\u0648\u0646 \u062F\u0641\u0646\u0643 \u0635\u0627\u062D\u064A\u0647 \u0641\u0627\u0647\u0645\u0647.\n\u0634\u0647\u062F \u0628\u062E\u0648\u0641:\u0633\u064A\u0628 \u0627\u064A\u062F\u064A \u0637\u064A\u0628.\n\u0635\u0642\u0631:\u0633\u0627\u0628 \u0623\u064A\u062F\u0647\u0627.\n\u0634\u0647\u062F \u0628\u0645\u0631\u062D \u0637\u0644\u0639\u062A \u0644\u0633\u0627\u0646\u0647\u0627 \u0644\u064A\u0647:\u0648\u0642\u0627\u0644\u062A \u0639\u064A\u0648\u0646\u0643 \u0645\u0648\u0632\u0629 \u0648\u0627\u0646\u062A \u0645\u0648\u0632 \u0628\u0633 \u0645\u0634 \u062A\u0633\u062A\u0627\u0647\u0644.\n\u0635\u0642\u0631 \u0628\u063A\u0636\u0628:\u0644\u0633\u0647 \u0647\u064A\u0645\u0633\u0643\u0647\u0627 \u0642\u0627\u0644\u062A \u0634\u0647\u062F \u062C\u0631\u064A \u0646\u0642\u0635 \u0627\u0644\u062C\u062F\u0639\u0646\u0647 \u0627\u062C\u0631\u0631\u0631\u0631\u064A \u064A\u0627\u0634\u0647\u062F\u062F\u062F\u062F\u062F\u062F\u062F\u062F \u0639\u0634\u0627\u0627\u0627\u0627\u0646 \u0627\u0646\u062A\u064A \u0639\u0627\u0648\u0632\u0629 \u062A\u062E\u0634 \u062F\u0646\u064A\u0627.\n\u0635\u0642\u0631 \u0641\u0644\u062A\u062A:\u0645\u0646\u0647 \u0636\u062D\u0643\u0647 \u0648\u0645\u0634 \u0645\u0646 \u0637\u0628\u0639\u0647 \u0648\u062F\u0631\u0627\u0647\u0627 \u0628\u0633\u0631\u0639\u0647 \u0642\u0628\u0644 \u0645 \u0641\u0627\u0631\u0633 \u064A\u0634\u0648\u0641\u0647\u0627.\n\n\n\u0641\u0627\u0631\u0633 \u0628\u0636\u062D\u0643:\u0627\u0644\u0628\u062A \u062F\u064A \u0639\u062B\u0644 \u0627\u0648\u064A \u0628\u0633 \u0627\u0646\u062A \u0627\u0632\u0627\u064A \u0633\u0628\u062A\u0647\u0627.\n\u0635\u0642\u0631 \u0628\u063A\u0636\u0628:\u0627\u0646\u062A \u062A\u062C\u0628\u0644\u064A \u062A\u0627\u0631\u064A\u062E\u0647\u0627 \u0645\u0646 \u0637\u0642 \u0637\u0642 \u0644\u0633\u0644\u0627\u0645\u0648\u0627 \u0639\u0644\u064A\u0643\u0648\u0627.\n\u0641\u0627\u0631\u0633:\u062D\u0627\u0636\u0631 \u064A\u0627\u0628\u0648\u0635.\n\u0635\u0642\u0631:\u0627\u0637\u0644\u0639 \u0639\u0644\u064A \u0634\u0631\u0643\u0647.\n\u201C\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u201D\u20