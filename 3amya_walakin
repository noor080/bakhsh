      "cpartOne": "_\u0645\u0639\u0642\u0648\u0648\u0648\u0648\u0644 \u062C\u0645\u064A\u0644\u0629\u061F\u061F\u061F!!!!!\n_\u0627\u0644\u062A\u0641\u062A \u062C\u0645\u064A\u0644\u0629 \u0644\u0644\u0635\u0648\u062A \u0627\u0644\u063A\u0631\u064A\u0628 \u0627\u0644\u0644\u0649 \u062C\u0627\u064A \u0645\u0646 \u0648\u0631\u0627\u0647\u0627 :- \u0627\u0646\u062A \u0645\u064A\u0646 \u061F!\n_\u0627\u0646\u0627 \u0639\u0627\u0645\u0631 \u0639\u0628\u062F \u0627\u0644\u0645\u0648\u0644\u0649 \u0631\u062C\u0644 \u0627\u0639\u0645\u0627\u0644\n_\u062C\u0645\u064A\u0644\u0629 \u0628\u0627\u0633\u062A\u063A\u0631\u0627\u0628:- \u0628\u0633 \u0627\u0646\u0627 \u0645\u0639\u0631\u0641\u0634 \u0645\u064A\u0646 \u062D\u0636\u0631\u062A\u0643\n_\u0639\u0627\u0645\u0631:- \u0628\u0633 \u0627\u0646\u0627 \u0627\u0639\u0631\u0641\u0643 \u0643\u0648\u064A\u0633\n_\u062C\u0645\u064A\u0644\u0629 :- \u0647\u064A \u0641\u0632\u0648\u0631\u0629 \u061F\u061F!!! \u0637\u0628 \u0648\u062D\u0636\u0631\u062A\u0643 \u0639\u0627\u064A\u0632 \u0627\u064A\u0647 \u062F\u0644\u0648\u0642\u062A\n_\u0639\u0627\u0645\u0631:- \u0623\u0647\u062F\u0649 \u0628\u0633 \u0627\u0646\u0627 \u0645\u0634 \u0639\u0627\u064A\u0632 \u062D\u0627\u062C\u0629 \u0648\u0627\u0644\u0644\u0647 \u0628\u0633 \u0634\u0648\u0641\u062A\u0643 \u0628\u062A\u0639\u064A\u0637\u0649 \u0642\u0648\u0644\u062A \u0627\u0634\u0648\u0641\u0643 \u0645\u0627\u0644\u0643 \u0648\u0628\u0639\u062F\u064A\u0646 \u0645\u0634 \u0627\u0644\u0645\u0641\u0631\u0648\u0636 \u062F\u0644\u0648\u0642\u062A\u0649 \u062A\u0643\u0648\u0646\u0649 \u0633\u0627\u0641\u0631\u062A\u0649 \u0639\u0634\u0627\u0646 \u0627\u0644\u0645\u0633\u0627\u0628\u0642\u0629\n_\u062C\u0645\u064A\u0644\u0629 \u0628\u062A\u0631\u064A\u0642\u0629:- \u0645\u0633\u0627\u0628\u0642\u0629 \u0645\u0627\u0647\u0649 \u062E\u0644\u0627\u0627\u0627\u0635 \u0643\u0644 \u062D\u0627\u062C\u0629 \u0631\u0627\u062D\u062A \u062C\u062A \u0639\u0644\u0649 \u062F\u0649 \u061F! \u0633\u0643\u062A\u