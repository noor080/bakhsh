      "cpartTwo": "_\u0645\u062D\u0645\u062F:- \u0627\u0646\u0627 \u0648\u0627\u0646\u0627 \u0628\u0631\u0628\u0649 \u0623\u064A\u0629 \u0639\u0644\u0649 \u0644\u0633\u0627\u0646\u0647\u0627 \u0648\u0639\u0645\u0627\u064A\u0644\u0647\u0627 \u0628\u0635\u0631\u0627\u062D\u0629 \u0627\u062A\u0639\u0644\u0642\u062A \u0628\u064A\u0647\u0627 \u0648\u0641 \u064A\u0648\u0645 \u0628\u0639\u062F \u0645\u0627\u0645\u0627\u0634\u064A\u062A \u0643\u0646\u062A \u0628\u0643\u0644\u0645 \u0646\u0641\u0633\u0649 \u0623\u0646\u0647\u0627 \u0644\u0648 \u0627\u062A\u0635\u0644\u062D\u062A \u0647\u062A\u062C\u0648\u0632\u0647\u0627 \u0641\u0643\u0627\u0646\u062A \u0641\u064A\u0647 \u0628\u0646\u062A \u0646\u0627\u0633\u064A\u0629 \u0627\u0644\u0628\u0627\u0644\u0637\u0648 \u0628\u062A\u0627\u0639\u0647\u0627 \u0648\u0627\u0644\u0628\u0646\u062A \u062F\u0649 \u064A\u0627\u0633\u064A\u062F\u0649 \u0643\u0627\u0646\u062A \u0639\u0627\u064A\u0632\u0629 \u062A\u062A\u062C\u0648\u0632\u0646\u0649 \u0623\u0645\u0627 \u0631\u062C\u0639\u062A \u062A\u0627\u062E\u062F \u0627\u0644\u0628\u0627\u0644\u0637\u0648 \u0633\u0645\u0639\u062A\u0646\u0649 \u0641\u0642\u0631\u0631\u062A \u062A\u062A\u062E\u0644\u0635 \u0645\u0646 \u0623\u064A\u0629 \u0639\u0634\u0627\u0646 \u0645\u062A\u062C\u0648\u0632\u0647\u0627\u0634 \u062E\u0644\u062A \u0628\u0646\u062A \u0645\u0628\u062A\u0639\u0631\u0641\u0634 \u062A\u062F\u0649 \u062D\u0642\u0646 \u0627\u0635\u0644\u0627 \u062A\u062F\u0649 \u062D\u0642\u0646\u0629 \u0644\u0637\u0641\u0644 \u0648\u0627\u062A\u0639\u0645\u062F\u062A \u062A\u0642\u0648\u0644\u0647\u0645 \u0639\u0644\u0649 \u0627\u0633\u0645\u0647\u0627 \u0648\u0628\u062F\u0627\u0644 \u0645\u0627\u062A\u0642\u0648\u0644 \u0627\u0633\u0645\u0647\u0627 \u0642\u0627\u0644\u062A \u0627\u0633\u0645 \u0623\u064A\u0629 \u0641\u0623\u0645\u0627 \u0627\u0644\u0623\u0647\u0644 \u0628\u0644\u063A\u0648\u0627 \u0639\u0637\u0648\u0627 \u0627\u0633\u0645 \u0623\u064A\u0629 \u0628\u0633 \u0627\u0644\u0645\u0644\u063