      "cpartTwo": "_\u0633\u0645\u0627\u062D:- \u0648\u0643\u0645\u0627\u0627\u0627\u0627\u0646 \u0628\u0642\u064A\u062A \u0648\u0644\u064A\u0629 \u061F\u061F!\u062D\u062A\u0649 \u0648\u0627\u0646\u062A\u0649 \u0639\u0645\u064A\u0629 \u0628\u0631\u0636\u0648\u0648\u0648\u0648\u0648 \u0644\u0633\u0627\u0646\u0643 \u0637\u0648\u0648\u0648\u0648\u064A\u0644 \u064A\u0627\u0632\u0628\u0627\u0627\u0627\u0627\u0627\u0627*\u0644\u0629\n_\u062C\u0645\u064A\u0644\u0629:- \u0627\u0646\u0627 \u0639\u0645\u0631\u0631\u0631\u0649 \u0645\u0627\u0643\u0646\u062A \u0632\u0628\u0627\u0627\u0627\u0627\u0627*\u0644\u0629 \u0648\u0627\u0628\u0646\u0643 \u0639\u0646\u062F\u0643 \u0627\u0647\u0648 \u0645\u0628\u0642\u0627\u0627\u0627\u0627\u0634 \u064A\u0644\u0632\u0645\u0646\u0649 \u0648\u0627\u0644\u0645\u0641\u0631\u0648\u0648\u0648\u0636 \u062A\u0631\u0648\u062D\u0649 \u062A\u0642\u0648\u0644\u0649 \u0644\u064A\u0647 \u0643\u062F\u0627 \u0645\u0634 \u0644\u064A\u0627\n_\u0633\u0645\u0627\u0627\u0627\u062D :- \u0627\u0646\u062A\u0649 \u0634\u0643\u0644\u0643 \u0647\u062A\u062A\u0639\u0628\u064A\u0646\u0627 \u0627\u0648\u0648\u0648\u0649 \u0648\u0644\u0627 \u0627\u0642\u0648\u0644\u0643 \u0647\u062A\u062A\u0639\u0628\u064A\u0646\u0627 \u0644\u064A\u064A\u064A\u0647 \u0645\u0646\u062A\u0649 \u062D\u0627\u0648\u0644\u062A\u0649 \u062A\u0646\u062A\u062D\u0631\u0649 \u064A\u0639\u0646\u0649 \u0644\u0648 \u0645\u0648\u0648\u0648\u062A\u0649 \u0645\u062D\u062F\u0634 \u0647\u064A\u0639\u0631\u0641 \u062D\u0627\u062C\u0629 \u0648\u0647\u064A\u0642\u0648\u0644\u0648 \u0627\u0646\u0643 \u0627\u0646\u062A\u062D\u0631\u062A\u0649 \u0647\u0627\u0627\u0627\u062A\u0649 \u064A\u0627\u0628\u062A \u064A\u0627\u0632\u0646\u064A\u0629 \u0627\u0644\u0645\u062E\u062F\u0629 \u062F\u0649\n_\u062C\u0645\u064A\u0644\u0629 :- \u0627\u0628\u0639\u062F\u062F\u062F\u062F\u062F\u062F\u0649 \u0639\u0646\u0649 \u0627\u0646\u0627 \u0645\u0639\u0631\u0641\u0634 \u062F\u0627 \u0643\u0644\u0644\u0644\u0647 \u0644\u064A\u06